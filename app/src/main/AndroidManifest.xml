<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="martin.prevencion" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".apiyoutube.VideoList"
            android:label="@string/title_activity_video_list" >
        </activity>
        <activity
            android:name="apiyoutube.VideoList"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/videolist_demo_name" >
            <meta-data
                android:name="@string/minVersion"
                android:value="13" />
            <meta-data
                android:name="@string/isLaunchableActivity"
                android:value="true" />
        </activity>
        <activity
            android:name=".SwipeTab"
            android:label="@string/title_activity_swipe_tab" >
        </activity>

    </application>

</manifest>
